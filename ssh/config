# ┏━┓┏━┓╻ ╻   ┏━╸┏━┓┏┓╻┏━╸╻┏━╸
# ┗━┓┗━┓┣━┫   ┃  ┃ ┃┃┗┫┣╸ ┃┃╺┓
# ┗━┛┗━┛╹ ╹   ┗━╸┗━┛╹ ╹╹  ╹┗━┛

Include private/*

VisualHostKey yes
host *
    controlmaster auto
    controlpath ~/.ssh/connections/ssh_%r@%h:%p
    controlpersist  yes
    ForwardX11 no
    KexAlgorithms curve25519-sha256@libssh.org,diffie-hellman-group-exchange-sha256

host space
    hostname inother.space
    port 2314
    user root
    compression no
    cipher blowfish-cbc
    IdentitiesOnly yes
    IdentityFile ~/.ssh/keys/space_rsa

Host tunnel
    HostName git.inother.space
    IdentityFile ~/.ssh/keys/space_rsa
    LocalForward 9906 127.0.0.1:3306
    IdentitiesOnly yes
    cipher arcfour
    User root

Host github.com
    HostName github.com
    user git
    IdentityFile ~/.ssh/keys/github_rsa
    IdentitiesOnly yes
    cipher arcfour


Host gitlab.com
    HostName gitlab.com
    user git
    IdentityFile ~/.ssh/keys/github_rsa
    IdentitiesOnly yes
    cipher arcfour

Host gist.github.com
    user git
    IdentitiesOnly yes
    IdentityFile ~/.ssh/keys/github_rsa
    cipher arcfour

host pi
    user pi
    hostname fsociety.local

host gate
    user root
    port 23147
    hostname 192.168.0.20
    cipher arcfour
    IdentitiesOnly yes
    IdentityFile ~/.ssh/keys/home_key

host bee
    user root
    hostname 51.255.162.140
    port 2314
    cipher arcfour
    IdentitiesOnly yes
    IdentityFile ~/.ssh/keys/home_key

host pirate
    user pirate
    hostname 192.168.0.32
    port 24728
    cipher arcfour
    IdentitiesOnly yes
    IdentityFile ~/.ssh/keys/home_key
