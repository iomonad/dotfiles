Include private/*

Host vgs vgs.42.fr vgse vgse.42.fr vogsphere vogsphere.42.fr vogsphere-exam vogsphere-exam.42.fr
	User vogsphere

VisualHostKey yes
host *
    controlmaster auto
    controlpath ~/.ssh/connections/ssh_%r@%h:%p
    controlpersist  yes
    ForwardX11 no
    IdentityFile ~/.ssh/keys/default_keys
    CheckHostIP yes
	ConnectTimeout 4
	ExitOnForwardFailure yes
	LogLevel VERBOSE
	PasswordAuthentication no
	NoHostAuthenticationForLocalhost yes
	PreferredAuthentications publickey
	Protocol 2,1
	PubkeyAuthentication yes
	VisualHostKey yes
    KexAlgorithms curve25519-sha256@libssh.org,diffie-hellman-group-exchange-sha256

Host *.42.fr
     SendEnv LANG LC_*
     StrictHostKeyChecking no
     GSSAPIAuthentication yes
     GSSAPIDelegateCredentials yes
     PasswordAuthentication yes

host space
    hostname inother.space
    port 2314
    user root
    compression no
    cipher blowfish-cbc
    IdentitiesOnly yes
    IdentityFile ~/.ssh/keys/space_rsa

