# Load X ressources
xrdb -load  $HOME/.Xresources
xset +fp ~/.fonts
xset fp rehash
xset -dpms
xrdb -load .Xresources
xsetroot -cursor_name left_ptr &
wmname LG3D
unclutter&
urxvtd --quiet --opendisplay --fork
autocutsel -selection clipboard -fork
hsetroot -tile .xres.d/wallpaper/chicago3.jpg
compton -c -t-5 -l-5 -r3 -o.48 &
#exec ~/documents/stumpwm/stumpwm
exec redhsift -l 48.886764:2.320873 # long / lat
exec xmonad
