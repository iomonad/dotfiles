# Load X ressources
xrdb -load  $HOME/.Xresources
xset +fp ~/.fonts
xset fp rehash
xset -dpms
xrdb -load .Xresources
xsetroot -cursor_name left_ptr &
wmname LG3D
urxvtd --quiet --opendisplay --fork
autocutsel -selection clipboard -fork
hsetroot -fill $HOME/.xres.d/wallpaper/GawjG.jpg
rm ~/.serverauth.*
#exec ck-launch-session dbus-launch --sh-syntax --exit-with-session xmobar $HOME/.xmonad/xmobar.hs
exec ck-launch-session dbus-launch --sh-syntax --exit-with-session xmonad
