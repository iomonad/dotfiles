# Load X ressources
xrdb -load  $HOME/.Xresources &
xset +fp ~/.fonts &
xset fp rehash &
xrdb -load .Xresources &
xsetroot -cursor_name left_ptr &
wmname LG3D &
unclutter -idle 2 &
urxvtd --quiet --opendisplay --fork &
autocutsel -selection clipboard -fork &
#autocutsel -s PRIMARY &
compton -c -t-5 -l-5 -r3 -o.48 &
pulseaudio --start -D &
xset r rate 220 60 &
#xinput disable 10 &
xset +dpms &
xset dpms 0 0 300 &
xset s noblank &
#xrandr --dpi 125 &
redshift -l 48.886764:2.320873 &
#stty -ixon &

# Set space cadets shift
xcape -e 'Shift_L=Shift_L|parenleft;Shift_R=Shift_R|parenright'
setxkbmap us dvorak &
xmodmap ~/.xmodmap

#exec xmonad
#exec stumpwm
exec i3 > /dev/null
#exec fvwm
