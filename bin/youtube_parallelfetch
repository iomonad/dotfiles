my $pm = Parallel::ForkManager->new( my $count = scalar(@u) );

printf("> Downloading %d videos in parallel...\n", $count);

for my $url(@u) {
  $pm->start and next;
  system('cclive', '-f', 'best', $url);
  $pm->finish;
}
$pm->wait_all_children;
