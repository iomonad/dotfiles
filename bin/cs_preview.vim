" define :commands for each of the available colorschemes:
"   :neverland
"   :github
"   :desert

"exe 'command ' . substitute(cs, '\v[^a-zA-Z0-9_]', '_', 'g')  . ' colorscheme ' . cs

" really weird stuff happened, it might be because I was doing
"   exe 'color ' . cs 
" here ':color' not being enough. investigate
silent! delcommand colorbuffer
silent! delcommand colordefault

" doing ctermfg=bg ctermbg=fg doesn't work - E420
let [new_fg, new_bg] = (&t_Co > 255)
    \ ? (&background == 'dark') ? [225, 232] : [232, 218]
    \ : (&background == 'dark') ? [7, 0]     : [0, 7]

for cs in map(
\  split(globpath(expand("$VIMRUNTIME"), "/colors/*.vim"), "\n"), 'fnamemodify(v:val, ":t:r")' 
\)
  exe printf(
    \ 'hi! clear | colorscheme %s | %s | %s | %s | %s | %s | %s | %s | %s | %s',
    \ cs,
    \ 'hi Normal         ctermfg=' . new_fg . 'ctermbg=' . new_bg ,
    \ 'hi ColorColumn    ctermfg=221 ctermbg=234 cterm=none       ',
    \ 'hi CursorColumn   ctermfg=233 ctermbg=124 cterm=boldreverse',
    \ 'hi CursorLine     ctermfg=000 ctermbg=250 cterm=reverse    ',
    \ 'hi LineNr         ctermfg=231 ctermbg=244 cterm=reverse   ',
    \ 'hi CursorLineNr   ctermfg=196 ctermbg=233 cterm=reverse  ',
    \ 'hi FoldColumn     ctermfg=067 ctermbg=232',
    \ 'redraw | colorscheme | hi Normal',
    \ 'sleep 900m',
  \)
endfor

